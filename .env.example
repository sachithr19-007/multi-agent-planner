# Multi-Agent Task Planning System - Environment Variables
# Copy this file to .env and fill in your values. Never commit .env.

# Application
APP_NAME="Multi-Agent Task Planning System"
DEBUG=false

# API
API_HOST=0.0.0.0
API_PORT=8000

# Logging (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# LangGraph (optional)
LANGGRAPH_CHECKPOINT_DIR=

# LLM (openai or anthropic)
LLM_PROVIDER=openai
LLM_MODEL=gpt-4o-mini

# LLM API Keys - set in .env, never commit
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
